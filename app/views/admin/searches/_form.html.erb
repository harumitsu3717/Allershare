<%= form_with url: admin_search_path, method: :get, local: true do |f| %>
  <%= f.text_field :content %>
  <%= f.select :model, options_for_select({ "ユーザー" => "user", "投稿" => "post", "タグ" => "tag"}) %>
  <%= f.select :method, options_for_select({ "完全一致" => "perfect", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partical"}) %>
  <%= f.submit '検索' %>
<% end %>